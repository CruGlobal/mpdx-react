        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "bit": {
        "env": {
            "compiler": "bit.envs/compilers/react-typescript@3.1.54",
            "tester": "bit.envs/testers/jest@22.4.3"
        },
        "componentsDefaultDirectory": "components/{name}",
        "packageManager": "npm",
        "overrides": {
            "*": {
                "dependencies": {
                    "@types/node": "^13.9.5"
                },
                "devDependencies": {
                    "@types/jest": "^25.1.5",
                    "@testing-library/jest-dom": "^5.3.0"
                }
            }
        }
    }
