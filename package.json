{
  "name": "mpdx-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "build:amplify": "AMPLIFY_NEXTJS_EXPERIMENTAL_TRACE=true next build",
    "serve": "next start",
    "disable-telemetry": "next telemetry disable",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:ci": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:ts": "tsc",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -s ./public",
    "test": "cross-env NODE_ICU_DATA=$(yarn bin node-full-icu-path) jest --silent",
    "test:log": "yarn test --silent=false",
    "test:watch": "cross-env NODE_ICU_DATA=$(yarn bin node-full-icu-path) jest --watch",
    "localtest": "yarn test --runInBand --verbose",
    "localtest:watch": "yarn test:watch --runInBand",
    "test:coverage": "cross-env NODE_ICU_DATA=$(yarn bin node-full-icu-path) jest --coverage",
    "chromatic": "chromatic",
    "extract": "i18next --config i18next-parser.config.js",
    "prettier:check": "prettier . --list-different",
    "prettier:write": "prettier . --write",
    "gql": "graphql-codegen --config codegen.yml",
    "gql:w": "yarn gql --watch",
    "postinstall": "husky install",
    "onesky:upload": "cross-env babel-node ./onesky/upload.js",
    "onesky:download": "cross-env babel-node ./onesky/download.js"
  },
